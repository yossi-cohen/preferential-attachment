  FormBox[
   SuperscriptBox["e", 
    RowBox[{"-", "x"}]], TraditionalForm]],
  FontSlant->"Italic",ExpressionUUID->
  "2e386fc6-9f1f-4066-8878-c1054a9677a0"],
 StyleBox[" = ",
  FontSlant->"Italic"],
