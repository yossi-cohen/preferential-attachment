Cell[BoxData[
 RowBox[{
  RowBox[{"RunExperiment", "[", 
   RowBox[{"dist_", ",", " ", "range_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "estimator", ",", " ", "g", ",", " ", "adjuster", ",", " ", "params", 
      ",", " ", "m", ",", " ", "samples", ",", " ", "\[Theta]", ",", " ", 
      OverscriptBox["\[Theta]", "^"], ",", " ", "e", ",", " ", "crb"}], "}"}],
     ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "generate", " ", "an", " ", "Estimator", " ", "for", " ", "dist"}], 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"estimator", " ", "=", " ", 
      RowBox[{"UniversalEstimator", "[", 
       RowBox[{"dist", ",", " ", "range"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Create", " ", "test", " ", "parameters", " ", "on", " ", "the", " ", 
       "unit", " ", 
       RowBox[{"range", " ", "[", 
        RowBox[{"0", ",", "1"}], "]"}], " ", "and", " ", "adjust", " ", 
       "them", " ", "to", " ", "the", " ", "original", " ", "range"}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"adjuster", "=", 
      RowBox[{"Adjuster", "[", 
       RowBox[{
        RowBox[{"range", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"range", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Theta]", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"adjuster", "[", "i", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "0.1", ",", "0.9", ",", "0.1"}], " ", "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"create", " ", "test", " ", "samples"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"m", " ", "=", " ", "256"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"\[Theta]_", ",", "m_"}], "]"}], " ", ":=", " ", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"dist", "[", "\[Theta]", "]"}], ",", "m"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"samples", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"g", "[", 
         RowBox[{
          RowBox[{"\[Theta]", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", " ", "m"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "9", ",", "1"}], " ", "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "predict", " ", "the", " ", "parameter", " ", "for", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      OverscriptBox["\[Theta]", "^"], " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"estimator", "[", 
         RowBox[{"samples", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "9", ",", "1"}], " ", "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"evaluate", " ", "prediction", " ", "errors"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"e", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Abs", "[", 
         RowBox[{
          RowBox[{"\[Theta]", "[", 
           RowBox[{"[", "i", "]"}], "]"}], " ", "-", " ", 
          RowBox[{
           OverscriptBox["\[Theta]", "^"], "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "9", ",", "1"}], " ", "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Compute", " ", 
        StyleBox["Cramer",
         FontSlant->"Italic"]}], 
       StyleBox["-",
        FontSlant->"Italic"], 
       RowBox[{
        StyleBox["Rao",
         FontSlant->"Italic"], 
        StyleBox[" ",
         FontSlant->"Italic"], 
        StyleBox["bound",
         FontSlant->"Italic"], 
        StyleBox[" ",
         FontSlant->"Italic"], "at", " ", 
        FormBox[
         SubscriptBox["\[Theta]", "i"],
         TraditionalForm]}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"crb", " ", "=", " ", 
      RowBox[{"CRB", "[", 
       RowBox[{"dist", ",", " ", "\[Theta]", ",", " ", "m"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "return", " ", "prediction", " ", "errors", " ", "and", " ", "crb"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"e", ",", " ", "crb"}], "}"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
