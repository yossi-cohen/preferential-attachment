Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateData", "[", 
   RowBox[{"n_", ",", " ", "m_", ",", " ", "f_", ",", " ", "range_", ",", " ", 
    RowBox[{"bins_", ":", 
     RowBox[{"-", "1"}]}]}], "]"}], ":=", " ", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "adjuster", ",", " ", "params", ",", "samples", ",", " ", "nbins"}], 
     "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "generate", " ", "an", " ", "adjuster", " ", "for", " ", "the", " ", 
      "parameter", " ", "range"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"adjuster", "=", 
      RowBox[{"Adjuster", "[", 
       RowBox[{
        RowBox[{"range", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", 
        RowBox[{"range", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "N", " ", "random", " ", "parameters", " ", "in", " ", 
       "the", " ", 
       RowBox[{"range", " ", "[", 
        RowBox[{"0", ",", "1"}], "]"}], " ", "and", " ", "adjust", " ", 
       "them", " ", "to", " ", "the", " ", "original", " ", "range"}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"params", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"adjuster", "[", 
         RowBox[{"RandomReal", "[", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1"}], "}"}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "N", " ", "samples", " ", "from", " ", "the", " ", 
       "generated", " ", "parameters"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"samples", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{
          RowBox[{"params", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", "m"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "create", " ", "a", " ", "histogram", " ", "for", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"nbins", "=", 
      RowBox[{"If", " ", "[", 
       RowBox[{
        RowBox[{"bins", ">", "0"}], ",", "bins", ",", 
        RowBox[{
         RowBox[{"Max", "[", "samples", "]"}], "+", "1"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "[", "x_", "]"}], ":=", 
      RowBox[{
       RowBox[{"HistogramList", "[", 
        RowBox[{"x", ",", " ", 
         RowBox[{"{", 
          RowBox[{"0", ",", "nbins", ",", "1"}], "}"}]}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"output", ":", " ", 
        RowBox[{"a", " ", "table", " ", "of", " ", "associations"}], ":", " ",
         "histogram"}], " ", "->", " ", 
       RowBox[{"param", " ", "over", " ", "all", " ", "samples"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"h", "@", 
         RowBox[{"samples", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}], "->", " ", 
        RowBox[{"params", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"i", ",", " ", "1", ",", " ", "n"}], "}"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
