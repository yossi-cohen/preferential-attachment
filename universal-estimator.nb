Cell["\<\
Fisher information for a distribution with a single parameter \[Theta]\
\>", "Text",
 CellChangeTimes->{
  3.850652087768042*^9},ExpressionUUID->"a5d383ba-66a6-4617-bea1-\
bb29a8f22006"],

Cell[BoxData[
 RowBox[{
  RowBox[{"FisherInformation", "[", "dist_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"f", ",", " ", "h"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"f", " ", "=", " ", 
      RowBox[{"PDF", "[", 
       RowBox[{
        RowBox[{"dist", "[", "\[Theta]", "]"}], ",", "x"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"h", "=", 
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{"Log", "[", "f", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Theta]", ",", "2"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Expectation", "[", 
      RowBox[{
       RowBox[{"-", "h"}], ",", 
       RowBox[{"x", "\[Distributed]", 
        RowBox[{"dist", "[", "\[Theta]", "]"}]}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.850652078087418*^9, 3.85065211034508*^9}, {
  3.850653249461179*^9, 3.850653249569304*^9}, {3.850653612487905*^9, 
  3.850653616253804*^9}, {3.85065425818961*^9, 3.850654265346849*^9}},
 CellLabel->"In[21]:=",ExpressionUUID->"fc7f8000-dfcc-41da-b976-9298d4051f5b"],

