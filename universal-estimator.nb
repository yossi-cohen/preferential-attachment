Cell[TextData[{
 StyleBox["The ", "Subsubsection"],
 StyleBox["UniversalEstimator", "Subsubsection",
  FontSlant->"Italic"]
}], "Subsubsection",
 CellChangeTimes->{{3.850658023805957*^9, 3.850658030162328*^9}, 
   3.850658341978902*^9},ExpressionUUID->"f5859950-70a7-44b3-8a50-\
bdb80e5a29e8"],

Cell[BoxData[
 RowBox[{
  RowBox[{"UniversalEstimator", "[", 
   RowBox[{"dist_", ",", " ", "range_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", " ", "n", ",", " ", "m", ",", " ", "trainSet", ",", " ", 
      "bins", ",", " ", "valSet", " ", ",", "net", ",", " ", "model", ",", 
      " ", "h"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "a", " ", "sample", " ", "generator"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"g", "[", 
       RowBox[{"\[Theta]_", ",", "m_"}], "]"}], " ", ":=", " ", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{
        RowBox[{"dist", "[", "\[Theta]", "]"}], ",", "m"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "train", " ", "a", " ", "NN", " ", "for", " ", "predicting", " ", "the",
        " ", "parameter", " ", "\[Theta]"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "generate", " ", "learning", " ", "samples", " ", "in", " ", "the", " ",
        "the", " ", "given", " ", "parameter", " ", "range"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"n", " ", "=", " ", "1000"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"number", " ", "of", " ", "learning", " ", "samples"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"m", " ", "=", " ", "256"}], ";", "   ", 
     RowBox[{"(*", 
      RowBox[{
      "number", " ", "of", " ", "observations", " ", "in", " ", "each", " ", 
       "sample"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"trainSet", "=", 
      RowBox[{"GenerateData", "[", 
       RowBox[{"n", ",", "m", ",", "g", ",", " ", "range"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"bins", " ", "=", " ", 
      RowBox[{"Length", "@", 
       RowBox[{
        RowBox[{"trainSet", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "use", " ", "the", " ", "same", " ", "number", " ", "of", " ", "bins", 
       " ", "for", " ", "the", " ", "validation", " ", "set"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"valSet", "=", 
      RowBox[{"GenerateData", "[", 
       RowBox[{
        RowBox[{"n", "/", "4"}], ",", "m", ",", "f", ",", " ", "range", ",", 
        " ", "bins"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Create", " ", "and", " ", "train", " ", "the", " ", "NN"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"net", " ", "=", " ", 
      RowBox[{"NetInitialize", "@", 
       RowBox[{"CreateNet", "[", "bins", "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"model", "=", 
      RowBox[{"NetTrain", "[", "\[IndentingNewLine]", 
       RowBox[{
       "net", ",", "\[IndentingNewLine]", "trainSet", ",", 
        "\[IndentingNewLine]", "All", ",", "\[IndentingNewLine]", 
        RowBox[{"Method", "->", "\"\<ADAM\>\""}], ",", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"ValidationSet", "\[Rule]", "valSet"}], ",", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"LossFunction", "\[Rule]", 
         RowBox[{"MeanAbsoluteLossLayer", "[", "]"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "define", " ", "a", " ", "function", " ", "for", " ", "generating", 
        " ", "a", " ", "histogram", " ", "with", " ", "nbins", " ", "from", 
        " ", "an", " ", "input", " ", "sample"}], " ", ")"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "[", "x_", "]"}], ":=", 
      RowBox[{
       RowBox[{"HistogramList", "[", 
        RowBox[{"x", ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "bins", ",", "1"}], "}"}]}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"return", " ", "an", " ", 
       StyleBox["Estimator",
        FontSlant->"Italic"]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Function", "[", 
      RowBox[{
       RowBox[{"{", "sample", "}"}], ",", " ", 
       RowBox[{"(", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "return", " ", "a", " ", "prediction", " ", "of", " ", "the", " ", 
          "parameter", " ", "\[Theta]"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"model", "[", 
         RowBox[{"h", "@", "sample"}], "]"}], "\[IndentingNewLine]", ")"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
